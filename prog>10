#include <stdio.h>

#define QTD 10
int main() 
{
  int nums[QTD], i, maiores_num[QTD] = {0}, maior=0;

  for(i=0;i<QTD;i++)
    {
    printf("Escreva o numero %d: \n", i+1);
    scanf("%d", &nums[i]);

      if(nums[i]>10)
      {
        maiores_num[i]=nums[i];
      maior++;
      }
    }
  puts("\n");
  for(i=0;i<QTD;i++)
    if(maiores_num[i]>10)
      printf("%d\n", maiores_num[i]);
  puts("\n");
  printf("%d numero(s) maior(es) que 10\n", maior);




  return 0;
}
